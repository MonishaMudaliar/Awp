ii. Test for prime numbers.
CODE: 
using System;
namespace Demo {
 class MyApplication {
 public static void Main() {
 int n, a = 0;
 n = Convert.ToInt32(Console.ReadLine());
 for (int i = 1; i <= n; i++) {
 if (n % i == 0) {
 a++;
 }
 }
 if (a == 2) {
 Console.WriteLine(n+" is a Prime Number");
 } else {
 Console.WriteLine("Not a Prime Number");
 }
 Console.ReadLine();
 }
 }
}
